.icons-sickrage {
    display: inline-block;
    vertical-align: middle;
    background-image: url('{{{spritesheet.escaped_image}}}');
    background-repeat: no-repeat;
}

{{#sprites}}
.icons-sickrage-{{name}} {
    @extend .icons-sickrage;
    background-position: {{px.offset_x}} {{px.offset_y}};
    width: {{px.width}};
    height: {{px.height}};
}
{{/sprites}}